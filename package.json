{
  "type": "module",
  "version": "1.0.3",
  "packageManager": "pnpm@9.7.1",
  "description": "A library for converting an HTML string into a JSX string using ASTs.",
  "author": "Clover You <2621869236@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/clovu/jsxify-html#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clovu/jsxify-html.git"
  },
  "bugs": "https://github.com/clovu/jsxify-html/issues",
  "keywords": [
    "html",
    "jsx",
    "react",
    "transformer",
    "converter"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "pnpm -C examples run dev",
    "build": "pnpm -r run build",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm -r publish --access=public",
    "test": "pnpm -r test",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@antfu/ni": "^0.22.4",
    "@antfu/utils": "^0.7.10",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@types/node": "^22.3.0",
    "bumpp": "^9.5.1",
    "eslint": "^9.9.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.9",
    "pnpm": "^9.7.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
